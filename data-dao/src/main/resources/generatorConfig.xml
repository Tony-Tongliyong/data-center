        <!--</jdbcConnection>-->
        <!-- 生成实体类的包名和位置 -->
        <javaModelGenerator targetPackage="com.shuzheng.legalpersondb.dao.entity" targetProject="C:\Users\54192\IdeaProjects\legal_person_db\data-dao\src\main\java"/>
        <!-- 生成的映射文件包名和位置 -->
        <sqlMapGenerator targetPackage="com.shuzheng.legalpersondb.dao.xml" targetProject="C:\Users\54192\IdeaProjects\legal_person_db\data-dao\src\main\java"/>
        <!-- 生成DAO的包名和位置 -->
        <javaClientGenerator targetPackage="com.shuzheng.legalpersondb.dao.mapper" targetProject="C:\Users\54192\IdeaProjects\legal_person_db\data-dao\src\main\java" type="XMLMAPPER"/>

